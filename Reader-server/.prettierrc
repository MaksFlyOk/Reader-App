{
	"trailingComma": "none",
	"tabWidth": 2,
	"useTabs": true,
	"semi": false,
	"singleQuote": true,
	"arrowParens": "avoid",
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true,
	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"/routes/",
		"/middleware/",
		"/utils/",
		"^../(.*)",
		"^./(.*)"
	],
	"plugins": ["@trivago/prettier-plugin-sort-imports"]
}
